snippet mg2
	margin:${1:1}px ${2:1}px;${3}
snippet mg4
	margin:${1:1}px ${2:1}px ${3:1}px ${4:1}px;${5}
#padding
snippet pd
	padding:${1:1}px;${2}
snippet pdt
	padding-top:${1:1}px;${2}
snippet pdr
	padding-right:${1:1}px;${2}
snippet pdb
