snippet ytransaction
	// <transaction>
	$dbConn = Yii::app()->db;

	if ($dbConn->getCurrentTransaction()) {
		$transaction = null;
	} else {
		$transaction = $dbConn->beginTransaction();
	}

	try {
		${1}

		if ($transaction) {
			$transaction->commit();
		}

	} catch (Exception $e) {
		if ($transaction) {
			$transaction->rollback();
		}

		throw $e;
	}
